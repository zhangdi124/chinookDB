<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee">
    <tlib-version>1.0</tlib-version>
    <short-name>Chinook Services</short-name>
    
    <tag>
        <description>
        	Queries into the track service
        </description>
        <name>queryTracks</name>
        <tag-class>com.chinookDB.tags.QueryTracksTagSupport</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <name>title</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>        
        <attribute>
            <name>artist</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>genre</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>    
        <attribute>
        	<name>sort</name>
        	<required>false</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>    
    </tag>
    
    <tag>
